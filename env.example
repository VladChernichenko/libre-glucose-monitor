# Backend Configuration
# Copy this file to .env and update with your values

# Backend API base URL (for authentication and user management)
REACT_APP_BACKEND_URL=https://libre-glucose-monitor-be.onrender.com

# COB API base URL (for carbs on board calculations)
REACT_APP_COB_API_URL=https://libre-glucose-monitor-be.onrender.com

# Libre LinkUp API base URL
REACT_APP_LIBRE_API_URL=https://api.libreview.com

# Nightscout Configuration
REACT_APP_NIGHTSCOUT_URL=https://vladchernichenko.eu.nightscoutpro.com
REACT_APP_NIGHTSCOUT_SECRET=your_nightscout_secret_here
REACT_APP_NIGHTSCOUT_TOKEN=your_nightscout_token_here

# CORS Proxy Configuration (for production)
REACT_APP_CORS_PROXY_URL=https://cors-anywhere.herokuapp.com

# Development mode (set to false for production)
REACT_APP_DOCKER=false

# Note: In production, ensure this is set to the correct API endpoint
# The application will use this URL for all API calls
